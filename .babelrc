{
  "stage": 0,
  "optional": [
    "runtime"
  ],
  "plugins": [
    "react-require"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-require",
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }
      }
    }
  }
}
